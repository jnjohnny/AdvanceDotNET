@model List<practice17._02._22.Models.Person>
@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_Loggedin.cshtml";
}

<h2>List</h2>

<table class="table-bordered">
    <tr>
        <th>Name</th>
        <th>Id</th>
        <th>Dob</th>
    </tr>
        @foreach (var p in Model)
            {
             <tr>
                    <td>@p.Name</td>
                    <td>@p.ID</td>
                    <td>@p.Dob</td>
              </tr>
            }

</table>

